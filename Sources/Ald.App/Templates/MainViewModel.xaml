<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModelsControls="clr-namespace:Ald.App.ViewModels.Controls"
                    xmlns:controls="clr-namespace:Ald.App.Views.Controls">

    <DataTemplate DataType="{x:Type viewModelsControls:GroupsViewModel}">
        <controls:GroupsView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModelsControls:SpecializationsViewModel}">
        <controls:SpecializationsView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModelsControls:DisciplinesViewModel}">
        <controls:DisciplinesView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModelsControls:EducationalPlansViewModel}">
        <controls:EducationalPlansView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModelsControls:TeachersViewModel}">
        <controls:TeachersView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModelsControls:AddTeachersViewModel}">
        <controls:TeacherAddView DataContext="{Binding}"/>
    </DataTemplate>

</ResourceDictionary>